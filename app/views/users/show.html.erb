<h1>User Profile</h1>

<p><strong>Username:</strong> <%= @user.username %></p>
<p><strong>Email:</strong> <%= @user.email %></p>

<ul>
  <li><%= link_to "Edit Profile", edit_user_path(@user) %></li>
  <li><%= link_to "Destroy", @user, method: :delete, data: { confirm: "Are you sure?" } %></li>
</ul>
